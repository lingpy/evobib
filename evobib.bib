@Article{Moran2011,
  author   = {Moran, Steven},
  title    = {{A}n ontology for accessing transcription systems},
  journal  = {Language Resources and Evaluation},
  year     = {2011},
  number   = {3},
  pages    = {345},
  url      = {http://dx.doi.org/10.1007/s10579-011-9158-8},
  volume   = {45},
