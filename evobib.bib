@Article{Willems2016,
  author   = {Willems, Matthieu and Lord, Etienne and Laforest, Louise and Labelle, Gilbert and Lapointe, François-Joseph and Di Sciullo, Anna Maria and Makarenkov, Vladimir},
  title    = {Using hybridization networks to retrace the evolution of Indo-European languages},
  journal  = {BMC Evolutionary Biology},
