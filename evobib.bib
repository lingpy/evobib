@InProceedings{Jaeger2015b,
  author       = {Jäger, Gerhard and List, Johann-Mattis},
  title        = {Factoring lexical and phonetic phylogenetic characters from word lists},
  booktitle    = {Proceedings of the 6th Conference on Quantitative Investigations in Theoretical Linguistics},
