@Article{Majid2021,
  author   = {Asifa Majid},
  journal  = {Trends in Cognitive Sciences},
  title    = {Human Olfaction at the Intersection of Language, Culture, and Biology},
  year     = {2021},
  doi      = {https://doi.org/10.1016/j.tics.2020.11.005},
