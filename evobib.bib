@Article{Bank2017,
  author   = {Bank, Sebastian},
  title    = {Segmentation: a remark on the Syncretism Principle},
  journal  = {Morphology},
  year     = {2017},
  number   = {1},
  pages    = {1--20},
  url      = {http://dx.doi.org/10.1007/s11525-016-9295-2},
