@Article{Metoz2006,
  author    = {Laurent Métoz},
  title     = {L’émergence de la lexicostatistique et de la glottochronologie en France au 19e s. : Constantine Samuel Rafinesque, Jules Dumont d’Urville et Paul Broca},
  journal   = {Histoire Épistémologie Langage},
  year      = {2006},
